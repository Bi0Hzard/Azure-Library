CashButton.Name = "CashButton"
CashButton.Parent = MainFrame
CashButton.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
CashButton.BorderColor3 = Color3.fromRGB(225, 128, 0)
CashButton.Position = UDim2.new(0.786377728, 0, 0.75014168, 0)
CashButton.Size = UDim2.new(0, 63, 0, 54)
CashButton.Font = Enum.Font.SourceSans
CashButton.Text = "💰"
CashButton.TextColor3 = Color3.fromRGB(0, 0, 0)
CashButton.TextSize = 45.000
CashButton.TextWrapped = true
CashButton.MouseButton1Down:connect(function()
	for _,_C in pairs(workspace['CurrentMap']['Coins']:GetChildren()) do
		_C.CanCollide = true
		local Character = workspace:FindFirstChild(game.Players.LocalPlayer.Name)
		if not Character then
			print('Your character does not exist, please try again when you have a character.')
		else
			_C.CFrame = Character.HumanoidRootPart.CFrame
		end
	end	
end)

