--https://www.roblox.com/games/5517325684/UP-Jumping-Legends

local library = loadstring(game:HttpGet("https://pastebin.com/raw/jyG6hWez", true))()
 
local GUI = library:CreateWindow({text='Jumping Legends'})
local TP = library:CreateWindow({text='Teleport ðŸ’¨'})


--  ANTI AFK
 
local VirtualUser=game:service'VirtualUser'
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            VirtualUser:CaptureController()
            VirtualUser:ClickButton2(Vector2.new())
end)

-- GUI

GUI:AddToggle("Auto Jump", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.jump = true 
        while wait(0) and _G.jump do
game.Players.LocalPlayer.Character.Humanoid.Jump = true
end

    else
        _G.jump = false
    end
end)

GUI:AddToggle("Auto Sell", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.sell = true 
        while wait(0) and _G.sell do
game:GetService("ReplicatedStorage")[".shared_Signals"].Sell:FireServer()

        end
    else
        _G.sell = false
    end
end)

GUI:AddButton("Infinity Jump", function() --you dont need "arg" for a button
loadstring(game:HttpGet("https://pastebin.com/raw/ujS74rsD", true))()
end)
 
GUI:AddButton("No Clip", function() --you dont need "arg" for a button
loadstring(game:HttpGet("https://pastebin.com/raw/bwZreMfj", true))()
end)
 
GUI:AddButton("Destroy Gui", function() --you dont need "arg" for a button
library.gui:Destroy()
end)
 
GUI:AddLabel("Creator: â˜£BioHazardâ˜£#9086")
 
pcall(function()
           for i = 1, 1 do
               game:GetService"StarterGui":SetCore('SendNotification', {
               Title = "Jumping Legends GUI";
               Text = "Created by â˜£BioHazardâ˜£#9086";
               Duration = 10;
                })
           end
        end)

