-->>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>EGGS<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<<
 
EggsX1:AddToggle("Auto Buy Chicken Eggs", function(arg) --the (arg) is if the checkbox is toggled or not
    if arg then
        _G.b = true 
        while wait(0) and _G.b do
local args = {
    [1] = 1,
    [2] = "buy3"
}

game:GetService("ReplicatedStorage").Remotes.BuyPets:InvokeServer(unpack(args))
        end
    else
        _G.b = false
    end
end)

