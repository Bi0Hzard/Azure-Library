function Sound11()
	local Song1 = Instance.new("Sound")

	Song1.Name = "Song1"
	Song1.SoundId = "rbxassetid://1846922161"
	Song1.Volume = 2
	Song1.archivable = false
	Song1.Parent = game.Workspace
	Song1:play()
end

